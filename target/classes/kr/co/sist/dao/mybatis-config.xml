<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="properties/database.properties"/>
<typeAliases>
<typeAlias type="day1224.EmpDTO" alias="eDTO"/>
<typeAlias type="day1226.EmpDTO" alias="empDTO"/>
<typeAlias type="day1226.EmpDomain" alias="empDomain"/>
<typeAlias type="day1226.ZipcodeDomain" alias="zipDomain"/>
<typeAlias type="day1226.EmpAllDomain" alias="eaDomain"/>
<!--  제조사, 모델명, 년식, 가격, 옵션, 배기량, 입력일을 저장하는 도메인 -->
<typeAlias type="day1226.CarModelDomain" alias="cmoDomain"/>
<typeAlias type="kr.co.sist.board.BoardDomain" alias="boardDomain"/>
<typeAlias type="kr.co.sist.board.RangeDTO" alias="rangeDTO"/>
<!-- 우편번호 검색 결과를 저장할 Domain  -->
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="oracle.jdbc.OracleDriver"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${id}"/>
        <property name="password" value="${pass}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="day1219/dept.xml"/> <!--  게시판 쿼리  -->
    <mapper resource="day1223/examMapper.xml"/> <!--  게시판 쿼리  -->
    <mapper resource="day1224/examMapper2.xml"/> <!--  게시판 쿼리  -->
    <mapper resource="day1224/examMapper3.xml"/> <!--  게시판 쿼리  -->
    <mapper resource="day1226/examMapper4.xml"/> <!--  게시판 쿼리  -->
    <mapper resource="day1229/examMapper5.xml"/> <!--  게시판 쿼리  -->

    <mapper resource="kr/co/sist/board/boardMapper.xml"/> <!--  자동차 업무  -->
    <mapper resource="kr/co/sist/car/carMapper.xml"/> <!--  자동차 업무  -->
  </mappers>
</configuration>